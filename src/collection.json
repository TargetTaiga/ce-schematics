{
	"$schema": "./node_modules/@angular-devkit/schematics/collection-schema.json",
	"schematics": {
		"ce-reducer": {
			"description": "A component reducer",
			"factory": "./reducer",
			"schema": "./reducer/schema.json"
		},
		"ce-effects": {
			"description": "A component effects",
			"factory": "./effects",
			"schema": "./effects/schema.json"
		},
		"ce-actions": {
			"description": "A component actions",
			"factory": "./actions",
			"schema": "./actions/schema.json"
		},
		"ce-constants": {
			"description": "A component constants",
			"factory": "./constants",
			"schema": "./constants/schema.json"
		},
		"ce-selectors": {
			"description": "A component selectors",
			"factory": "./selectors",
			"schema": "./selectors/schema.json"
		},
		"ce-state": {
			"description": "A component state",
			"factory": "./state",
			"schema": "./state/schema.json"
		}
	}
}
